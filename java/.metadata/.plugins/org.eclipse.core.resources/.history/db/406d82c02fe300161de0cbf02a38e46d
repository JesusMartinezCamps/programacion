package org.foobarspam.esBisiesto.year;

public class Year {

	private int year = 0;
	
	/*Constructor que recibe un aÃ±o*/
	public Year(int year){
		this.year = year;
	}
	
	/*getters y setters*/
	public int getYear(){
		return this.year;
	}
	
	/*El metodo esBisiesto*/
	public Boolean esBisiesto(){
		return getYear() % 4 == 0;
	}
}
