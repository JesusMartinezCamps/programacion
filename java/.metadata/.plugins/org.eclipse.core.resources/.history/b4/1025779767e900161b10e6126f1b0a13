package org.foobarspam.punto;

public class MatrizPuntoSilla {

	private int total = 0;
	/*Constructor*/
	public MatrizPuntoSilla(){
		
	}
	
	/*Getter setters*/
	
	public int getTotal(){
		return this.total;
	}
	public void setTotal(int total){
		this.total = total;
	}
	public int recorrerMatriz(){
		
		return total;
	}
}
