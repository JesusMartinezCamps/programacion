package foobarspam;

public class Rectangulo extends FiguraGeometrica {
	
	
	private double ladoA = 4;
	private double ladoB = 3;
	private String nombre = "Rectangulo" ;
	
	/*Constructor sin parametros*/
	public Rectangulo(){
		super(); /*se invoca al constructor de la clase superior*/
	}
	
	/*Constructor con los dos paramtros*/
	public Rectangulo(double ladoA, double ladoB){
		super();
		this.ladoA = ladoA;
		this.ladoB = ladoB;
	}
	
	/*Constructor con los dos parametros m√°s el string de nombre*/
	public Rectangulo(String nombre, double ladoA, double ladoB){
		super(nombre);
		this.ladoA = ladoA;
		this.ladoB = ladoB;
	}
	
}
